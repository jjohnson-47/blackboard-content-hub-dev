#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/verlyn13/projects/blackboard-content-hub-dev/node_modules/.pnpm/vite@6.2.2_@types+node@22.13.10_terser@5.39.0/node_modules/vite/bin/node_modules:/home/verlyn13/projects/blackboard-content-hub-dev/node_modules/.pnpm/vite@6.2.2_@types+node@22.13.10_terser@5.39.0/node_modules/vite/node_modules:/home/verlyn13/projects/blackboard-content-hub-dev/node_modules/.pnpm/vite@6.2.2_@types+node@22.13.10_terser@5.39.0/node_modules:/home/verlyn13/projects/blackboard-content-hub-dev/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/verlyn13/projects/blackboard-content-hub-dev/node_modules/.pnpm/vite@6.2.2_@types+node@22.13.10_terser@5.39.0/node_modules/vite/bin/node_modules:/home/verlyn13/projects/blackboard-content-hub-dev/node_modules/.pnpm/vite@6.2.2_@types+node@22.13.10_terser@5.39.0/node_modules/vite/node_modules:/home/verlyn13/projects/blackboard-content-hub-dev/node_modules/.pnpm/vite@6.2.2_@types+node@22.13.10_terser@5.39.0/node_modules:/home/verlyn13/projects/blackboard-content-hub-dev/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vite@6.2.2_@types+node@22.13.10_terser@5.39.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../.pnpm/vite@6.2.2_@types+node@22.13.10_terser@5.39.0/node_modules/vite/bin/vite.js" "$@"
fi
